{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": [
              "node 6",
              "> 1%"
            ]
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },

    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": [
              "node 6",
              "> 1%"
            ]
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },

    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": [
              "node 6",
              "> 1%"
            ]
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-optional-chaining"
      ]
    }
  }
}
